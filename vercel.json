{
  "version": 2,
  "buildCommand": "npm install && npm run vercel-simple-build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "framework": null,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Pragma": "no-cache",
        "Expires": "0"
      }
    },
    {
      "src": "/(assets|images|fonts)/(.*)",
      "dest": "/dist/public/$1/$2",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.(js|css))",
      "dest": "/dist/public/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
      "dest": "/dist/public/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/api"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}